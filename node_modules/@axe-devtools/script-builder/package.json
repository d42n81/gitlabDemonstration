{
  "name": "@axe-devtools/script-builder",
  "version": "4.6.1",
  "description": "API for accessibility testing and reporting within Node.js-based applications",
  "main": "dist/script-builder.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist/get-axe-config.d.ts",
    "dist/index.d.ts",
    "dist/types.d.ts"
  ],
  "scripts": {
    "prepare": "npm run build",
    "create-standards": "attest-standards -d src/ruleset --508=508.js --wcag2=default.js --wcag21=wcag21.js --wcag22=wcag22.js --wcag2aaa=wcag2aaa.js --wcag21aaa=wcag21aaa.js --wcag22aaa=wcag22aaa.js",
    "prepublishOnly": "npm run build",
    "pretest": "npm run create-standards && rimraf test/tmp",
    "test": "mocha -r ts-node/register --timeout 60000 test/*.ts",
    "build": "npm run create-standards && webpack --bail",
    "coverage": "nyc npm run test"
  },
