    }
  ],
  "devDependencies": {
    "@deque/attest-standards": "^4.6.1",
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.1",
    "@types/rimraf": "^3.0.2",
    "@types/webpack": "^5.28.0",
    "axe-core": "^4.6.3",
    "chai": "^4.3.6",
    "jfrog-cli-go": "^1.53.4",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "raw-loader": "^4.0.2",
    "require-from-string": "^2.0.2",
