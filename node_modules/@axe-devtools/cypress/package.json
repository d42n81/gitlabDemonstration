{
  "name": "@axe-devtools/cypress",
  "version": "4.6.1",
  "description": "Axe DevTools Cypress Integration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsc",
    "prebuild": "rimraf dist",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "cy:run": "cypress run",
    "test": "npm run cy:run",
    "coverage": "nyc npm run test",
    "postbuild": "esbuild src/plugin/analytics.ts --bundle --outfile=dist/plugin/analytics.js --platform=node --minify --target=node14"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "@axe-devtools/logger": "^4.5.0",
    "@axe-devtools/script-builder": "^4.6.1",
    "axe-core": "^4.6.3",
    "table": "^6.8.0"
  },
  "devDependencies": {
    "@deque/metrics-library": "^3.4.0",
    "@types/cypress": "^1.1.3",
    "@types/node": "^18.0.3",
    "@types/table": "^6.0.0",
    "ally.js": "^1.4.1",
    "chai": "^4.3.6",
    "cross-env": "^7.0.3",
    "cypress": "^11.1.0",
    "cypress-shadow-dom": "^1.4.1",
    "esbuild": "^0.15.7",
    "mocha": "^10.0.0",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "cypress": "4 - 11"
  },
  "nyc": {
    "checkCoverage": true,
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "statements": 100,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "exclude": [
      "dist"
    ]
  },
  "gitHead": "7424f054df4b3d531be0c4b5bf4ef4b3705997f0"
}
