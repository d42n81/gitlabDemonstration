{
  "name": "@axe-devtools/reporter",
  "version": "4.5.1",
  "description": "Program for building HTML and XML reports from AxeDevTools output",
  "main": "dist/reporter.js",
  "typings": "dist/index.d.ts",
  "license": "UNLICENSED",
  "files": [
    "dist/index.d.ts",
    "dist/Reporter.d.ts",
    "dist/client/bundle.css",
    "dist/client/bundle.js",
    "dist/client/index.html"
  ],
  "scripts": {
    "dev": "webpack-dev-server --port=3000",
    "build:client": "webpack --bail --config=webpack/client.ts",
    "build:node": "webpack --bail --config=webpack/node.ts",
    "prebuild": "rimraf dist",
    "build": "npm run build:node && npm run build:client",
    "lint": "tslint --project tsconfig.json 'src/**/*.{ts,tsx}'",
    "test": "mocha -r ts-node/register -r jsdom-global/register -r ignore-styles -r src/client/testutils/enzyme.ts \"src/**/*.test.{ts,tsx}\" --exit",
    "coverage": "nyc npm run test",
    "prepare": "cross-env NODE_ENV=production npm run build"
  },
  "devDependencies": {
    "@axe-devtools/logger": "^4.5.1",
    "@axe-devtools/reporter-csv": "^4.5.1",
    "@deque/cauldron-react": "^3.0.1",
    "@deque/cauldron-styles": "^3.0.1",
    "@types/chai": "^4.2.21",
    "@types/cheerio": "^0.22.30",
    "@types/classnames": "^2.3.1",
    "@types/commander": "^2.12.2",
    "@types/enzyme": "^3.10.9",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/execa": "^0.9.0",
    "@types/he": "^1.1.2",
    "@types/html-webpack-plugin": "^3.2.6",
    "@types/jsdom": "^12.2.1",
    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.17.60",
    "@types/optimize-css-assets-webpack-plugin": "^1.3.3",
    "@types/papaparse": "^5.3.2",
    "@types/prismjs": "^1.16.6",
    "@types/react": "^17.0.16",
    "@types/rimraf": "^2.0.5",
    "@types/sinon": "^7.0.6",
    "@types/tempy": "^0.2.0",
    "@types/terser-webpack-plugin": "^1.2.0",
    "@types/webpack": "^4.41.30",
    "autoprefixer": "^9.4.6",
    "awesome-typescript-loader": "^5.2.1",
    "axe-core": "^4.5.2",
    "cauldron-react": "^4.0.2",
    "chai": "^4.3.4",
    "cheerio": "^1.0.0-rc.10",
    "classnames": "^2.3.1",
    "commander": "^2.20.3",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "deque-pattern-library": "^4.2.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "execa": "^1.0.0",
    "fast-xml-parser": "^3.19.0",
    "file-loader": "^3.0.1",
    "font-awesome": "^4.7.0",
    "he": "^1.2.0",
    "html-webpack-plugin": "^3.2.0",
    "ignore-styles": "^5.0.1",
    "jfrog-cli-go": "^1.50.2",
    "jsdom": "^14.1.0",
    "jsdom-global": "^3.0.2",
    "keycode": "^2.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "mkdirp": "^0.5.5",
    "mocha": "^5.2.0",
    "normalize.css": "^8.0.1",
    "nyc": "^13.1.0",
    "optimize-css-assets-webpack-plugin": "^5.0.8",
    "papaparse": "^5.3.1",
    "pkg": "^4.5.1",
    "postcss-discard-font-face": "^3.0.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "prismjs": "^1.24.1",
    "react": "^16.14.0",
    "react-dom": "^16.3.1",
    "react-wai-accordion": "0.0.0-canary.2ad4b10f",
    "rimraf": "^2.6.3",
    "sinon": "^7.2.4",
    "style-loader": "^0.23.1",
    "tempy": "^0.2.1",
    "ts-node": "^8.0.1",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^4.2.4",
    "url-loader": "^1.1.2",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.11.2"
  },
  "nyc": {
    "checkCoverage": true,
    "exclude": [
      "**/*.test.ts",
      "**/*.test.tsx",
      "src/client/testutils/*.ts",
      "src/testutils/*.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true,
    "statements": 80,
    "branches": 80,
    "functions": 80,
    "lines": 80
  },
  "pkg": {
    "assets": [
      "dist/client/*"
    ],
    "targets": [
      "node12-linux-x64",
      "node12-macos-x64",
      "node12-win-x64"
    ]
  },
  "gitHead": "8b58d20ad41135afbdab1ed69050860980c832f8"
}
